<html dir="ltr">
 <head>
   {% include 'head' %}
   <title>{{ search|capitalize }} - Radio Esperanzah! 2010</title>
 </head>
 <body class="search">
   {% include 'header' %}
   <div id="global">
   <div id="box">
    <div id="breadcrumb">
     <p>Où suis-je ?
      <span class="breadcrumbs pathway">
      <a href="{{ show.url_for }}" class="pathway">Radio Esperanzah!</a> - {{ search|capitalize }}
      </span>
     </p>
    </div>
    <div id="menu">
     <ul id="main-menu">
      <li class="item1">
       <a href="http://radio.esperanzah.be"><span>Accueil</span></a>
      </li>
     </ul>
    </div>
    <div id="content">
     <div id="contentarea2"><a name="content"></a>
      <div id="wrapper">
       <div id="main2">
        <h1 class="componentheading">Toutes les émissions étiquetées : ‘{{ search }}’</h1>
        <div class="blog">
         {% for episode in episodes %}
          {% include 'episode' %}
         {% endfor %}
         <p class="play-selection"><a href="/tags/{{search}}?format=m3u">&#8594; Ecouter cette sélection d'émissions</a></p>
        </div>
       </div>
      </div>
     <div id="right">
      {% include 'sidebar' %}
     </div>
    </div>
    <div id="poupee-sax"></div>
    <div id="poupee-bras"></div>
    <div id="menu-left"></div>
    <div id="menu-right"></div>
    <div id="feuille"></div>
   </div>
   <div id="partners"></div>
  </div>
  {% include 'footer' %}
 </body>
</html>